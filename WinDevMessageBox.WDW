     procedure_id : 1499575491988225758
     type_code : 14
     code : |1+
      procedure SoundWarning()
      // Emits a beep like the system
      
      
      
      <if ExecutionTarget=WindowsApplication>
      	switch nType
      		case 0 : // Info
      			CallDLL32("USER32","MessageBeep", MB_ICONASTERISK)
      		case 1 : // Error
      			CallDLL32("USER32","MessageBeep", MB_ICONHAND)
      		case 2 : // YesNo
      			CallDLL32("USER32","MessageBeep", MB_ICONQUESTION)
      		case 3 : // Confirm
      			CallDLL32("USER32","MessageBeep", MB_ICONQUESTION)
      		case 4 : // Warning
      			CallDLL32("USER32","MessageBeep", MB_ICONASTERISK)
      		case 5 : // OKCancel
      			CallDLL32("USER32","MessageBeep", MB_ICONQUESTION)				
      	end
      	
      <else if CibleExécution=Java>
      	
      	Beep()
      	
      <end>
      
      
