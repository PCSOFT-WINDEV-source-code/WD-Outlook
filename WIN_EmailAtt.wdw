  procedures :
   -
     name : ExtractAttachment
     internal_properties : CAAAAAgAAACK5Zbgb1cAEeiYNyGGBHu+uUb+ClbRHcYanPQBkeoKG0wJi7oplDBJQmo22E746a7q0uaDdmg1k5JO3Qb8rtThGRooDKgo3y4AqJIDKxAfREVKuoy8sT5dO2iTcy6zA6qqWXoqA8RoIxfCfZ5HxxpH4EZcDfust8y3PKvnDBzMARHTiwmqBUwp1/R2ftO3YhjLpLe2aWI=
     procedure_id : 1407598303476046752
     type_code : 14
     code : |1-
      // Summary: <specify the action of the procedure>
      // Syntax:
      //[ <Result> = ] ExtractAttachment (<sNameAT> [, <sTempDir>])
      //
      // Parameters:
      //	sNameAT: <Name of the attachment>
      // 	sTempDir (default value=""): <specify the role of sTempDir>
      // Return value:
      // 	string: 
      //
      procedure ExtractAttachment(sATName,sTempDir="")
      
      when exception in
      	if  sTempDir~="" then sTempDir=gsTempDirATT
      	
      	// Reposition on the corresponding OutLook object if we are not creating a new message but if we are reading an old one
      	if Email.ID <> "" then
      		if not	EmailReadMessage(SessionID,Email.ID) then
      			Error(<§$0006§>,ErrorInfo(errMessage))
      			return ""
      		end
      		
      	end
      	
      	
      	// Find the requested attachment
      	sNameAttachedFile	is string
      	sNameCopiedFile		is string 
      	nNbMsg				is int	= Email.NbAttach
      	
      	for i = 1 to nNbMsg
      		sNameAttachedFile = Email.Attach[i]
      		if sNameAttachedFile ~= sATName then
      			// Extraction into the current directory
      			sNameCopiedFile=sTempDir + sNameAttachedFile
      			EmailSaveAttachment(sNameAttachedFile,sNameCopiedFile) 
      			return sNameCopiedFile
      		end
      	end
      	
      	return ""
      	
      do
      	Error(ExceptionInfo(errSummary))
      	ReturnToCapture()
      end
      
      return ""
     type : 458752
