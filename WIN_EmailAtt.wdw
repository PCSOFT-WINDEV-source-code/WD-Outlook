   -
     code : |1-
      procedure WIN_EmailAtt()
      
      
      
      // Display the list of attachments
      // Note: 
      // ATT=incoming attachments
      // AT2=attachments to send (by default: the received ones)
      
      for i=1 _to_ Email.NbAttach
      	ListAdd(LIST_Attachments,Email.Attach[i])		
      end
