        code : |1+
         // Selecting the file
         sFileName is string
         sFileName = fSelect("","",<§$0000§>,<§$0001§>+TAB+"*.*","*",fselOpen+fselExist)
         if sFileName~="" then return
         
         sLastName is string 
         sLastName = fExtractPath(sFileName,fFile+fExtension) // File name without path
         
         // Already found in the list
         if ListSeek(LIST_Attachments,sLastName)>0 then return
         
         // Add
         AddAttachment(sFileName)
         ListAdd(LIST_Attachments,sLastName)
         
